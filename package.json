    "description": "Snippet for function component in react by typescript",
    "keywords": [
        "snippets",
        "react",
        "javascript",
        "typescript",
        "hooks",
        "function component"
    ],
    "license": "MIT",
    "version": "0.0.3",
    "publisher": "Hans",
    "repository": {
        "type": "git",
